<ul class="pagination">
  {% if paginate.previous %}
  <li class="prev"><a href="{{ paginate.previous.url }}" title="{{ paginate.previous.title }}"><i class="fa fa-caret-left" ></i> <span class="d-none">{{ 'general.pagination.prev' | t}}</span></a></li>
  {% else %}
    <li class="disabled"><span><i class="fa fa-caret-left" ></i></span></li>
  {% endif %}

  {% for part in paginate.parts %}
    {% if part.is_link %}
      <li>
        <a href="{{ part.url }}" title="">{{ part.title }}</a>
      </li>
    {% else %}
      {% if part.title == paginate.current_page %}
        <li class="active"><span>{{ part.title }}</span></li>
      {% else %}
        <li><span>{{ part.title }}</span></li>
      {% endif %}
    {% endif %}
  {% endfor %}

  {% if paginate.next %}
  <li class="next"><a href="{{ paginate.next.url }}" title="{{ paginate.next.title }}"><span class="d-none">{{ 'general.pagination.next' | t}}</span><i class="fa fa-caret-right" ></i></a></li>
  {% else %}
    <li class="disabled"><span><i class="fa fa-caret-right" ></i></span></li>
  {% endif %}
</ul>